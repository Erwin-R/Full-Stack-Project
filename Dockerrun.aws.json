{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "potatochip-api",
      "image": "zer0beans/potatochip-api:16.12.2024.03.36.17",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 8080
        }
      ],
      "environment": [
        {
          "name": "SPRING_PROFILES_ACTIVE",
          "value": "test"
        }
      ]
    }
  ]
}

